{
  "name": "sample-front-end",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run local",
    "local": "vue-cli-service serve --mode test",
    "build": "vue-cli-service build --mode prod"
  },
  "dependencies": {
    "axios": "0.21.1",
    "clipboard": "2.0.8",
    "core-js": "3.15.1",
    "element-ui": "2.13.2",
    "ethers": "5.7.0",
    "jquery": "3.6.0",
    "js-yaml": "^3.14.0",
    "lodash": "4.17.21",
    "pubsub-js": "1.9.0",
    "tearust_utils": "1.0.3",
    "vue": "2.6.14",
    "vue-router": "3.3.4",
    "vuex": "3.6.0",
    "@polkadot/util": "10.1.9"
  },
  "devDependencies": {
    "@open-wc/webpack-import-meta-loader": "https://github.com/liyangwood/webpack-import-meta-loader-fixed.git",
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "babel-eslint": "10.1.0",
    "eslint": "7.2.0",
    "eslint-plugin-vue": "6.2.2",
    "sass": "1.35.1",
    "sass-loader": "8.0.2",
    "sass-resources-loader": "2.2.3",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-async-promise-executor": 1
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
